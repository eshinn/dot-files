{
  "diagnostic.displayByAle": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.quitOnOpen": true,
  "eslint.autoFixOnSave": true,
  "javascript.suggest.autoImport": true,
  "typescript.suggest.autoImport": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "dart",
    "markdown",
    "typescript",
    "javascript"
  ],
  "vetur.ignoreProjectWarning": true,
  "yaml.customTags": [
    "!And sequence",
    "!Base64 scalar",
    "!Cidr scalar",
    "!Condition scalar",
    "!Equals sequence",
    "!FindInMap sequence",
    "!GetAtt scalar",
    "!GetAZs scalar",
    "!If sequence",
    "!ImportValue scalar",
    "!Join sequence",
    "!Not sequence",
    "!Or sequence",
    "!Ref scalar",
    "!Select sequence",
    "!Split sequence",
    "!Sub scalar",
    "!Transform mapping"
  ]
}
